{
  "name": "znotion",
  "version": "0.1.0",
  "description": "Cliente Node.js elegante e modular para integra√ß√£o com a API do Notion",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "standard-version": {
    "types": [
      {"type": "feat", "section": "‚ú® Novas Funcionalidades"},
      {"type": "fix", "section": "üêõ Corre√ß√µes de Bugs"},
      {"type": "docs", "section": "üìö Documenta√ß√£o"},
      {"type": "style", "section": "üíé Estilos"},
      {"type": "refactor", "section": "‚ôªÔ∏è Refatora√ß√£o"},
      {"type": "perf", "section": "‚ö°Ô∏è Performance"},
      {"type": "test", "section": "‚úÖ Testes"},
      {"type": "build", "section": "üì¶ Build"},
      {"type": "ci", "section": "üë∑ CI"},
      {"type": "chore", "section": "üîß Chore"}
    ],
    "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commit/{{hash}}",
    "compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/compare/{{previousTag}}...{{currentTag}}",
    "issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/issues/{{id}}",
    "userUrlFormat": "{{host}}/{{user}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seu-usuario/znotion.git"
  },
  "keywords": [
    "notion",
    "api",
    "client",
    "typescript",
    "rest",
    "notion-api",
    "notion-client",
    "notion-typescript"
  ],
  "author": "Seu Nome <seu.email@exemplo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seu-usuario/znotion/issues"
  },
  "homepage": "https://github.com/seu-usuario/znotion#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.14",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "express-joi-validation": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "standard-version": "^9.3.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "@types/joi": "^17.2.3"
  }
}
